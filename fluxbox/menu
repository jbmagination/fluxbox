     [exec] () {}
[submenu] (Fluxbox) </home/jbm/.icons/fluxbox.png>
    [config] (Config)
    [workspaces] (Workspace list)
    [commanddialog] (Fluxbox command)
    [exec] (Fluxbox folder) {/usr/bin/thunar ~/.fluxbox/}
    [reconfig] (Reload config)
    [restart] (Restart Fluxbox)
[end]
    [exec] () {}
    [exec] (Lock) {xautolock -locknow} </home/jbm/.icons/lock.png>
    [exit] (Exit) {/home/jbm/.fluxbox/shutdown} </home/jbm/.icons/exit.png>
    [exec] (Shutdown) {shutdown -P now} <~/.icons/exit.png>
    [exec] (Restart) {shutdown --reboot now}
    [exec] (Halt) {shutdown -H now} <~/.icons/exit.png>
  [endencoding]
[end]
